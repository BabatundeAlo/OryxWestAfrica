@model IEnumerable<OryxWestAfrica.Models.Partner>

@{
    ViewData["Title"] = "Index";
}

@*<h2>Index</h2>*@



<div class="container">
    <div class="row"></div>
    <div class="row"></div>
    <div class="row"></div>
    <div class="blue-grey lighten-4 black-text darken-text-2 center">
        <div></div>
        <p><h3>PARTNER</h3></p>
    </div>

    @*<div class="container">*@
    <div class="row"></div>
    <div class="row"></div>
    <div class="row"></div>




    <p>
        <a asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                @*
                <th>
                    @Html.DisplayNameFor(model => model.Image)
                </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.Details)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                @*
                <td>
                    @Html.DisplayFor(modelItem => item.Image)
                </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.Details)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.PartnerID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.PartnerID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.PartnerID">Delete</a>
                </td>
            </tr>
            }
        </tbody>
    </table>





</div>