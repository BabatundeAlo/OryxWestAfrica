

<!DOCTYPE html>
<html>
<head>
   

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <environment include="Development">

        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />


        <!--Import materialize.css-->
        <link type="text/css" rel="stylesheet" href="~/css/materialize.css" media="screen,projection" />
        <link href="~/css/style.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/css/materialize.css"
              asp-fallback-href="~/css/materialize.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
</head>

<body>
    <header>
        <nav class="purple darken-4 fixed">
            <div class="nav-wrapper">

                <ul id="nav-mobile" class="right hide-on-med-and-down purple-text darken-4">
                    <li><a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">HOME</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="About" class="navbar-brand">ABOUT US</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Partner" class="navbar-brand">PARTNERS</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Solution" class="navbar-brand">SOLUTIONS</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Client" class="navbar-brand">CLIENTS</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Support" class="navbar-brand">SUPPORT</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Career" class="navbar-brand">CAREER</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Gallery" class="navbar-brand">GALLERY</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Contact" class="navbar-brand">CONTACT US</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Admin" class="navbar-brand"><i class="material-icons left">person</i>Admin</a></li>
                </ul>
                <p>
                    <a href="#" class="brand-logo"><img src="~/images/oryxweb logo.jpg" class="imgHW" /></a>
                </p>
            </div>
        </nav>
    </header>
    @RenderBody()





    @*starting point of footer of the page*@
    <footer class="page-footer purple darken-4">
        <h3 class="centered center-align white deep-purple-text">REACH US</h3>
        <div class="container">
            <div class="row">
                <div class="col l4 m6 s12">
                    <h5 class="white-text">Want to be kept updated?</h5>
                    <p>Subscribe to our newsletter and stay up to date with the latest news and deals!</p>

                    <div class="input-field col s12">
                        <input id="email" type="email" class="validate">
                        <label for="email">Email</label>
                    </div>

                    <div class="input-field col s6">
                        <input id="first_name" type="text" class="validate">
                        <label class="active" for="first_name2">First Name</label>
                    </div>
                    <div class="input-field col s6">
                        <input id="last_name" type="text" class="validate">
                        <label class="active" for="first_name2">Last Name</label>
                    </div>
                    <button class="btn waves-effect waves-light" type="submit" name="action">
                        Submit
                        <i class="material-icons right">send</i>
                    </button>
                </div>
                <div class="col l4 m6 s12">
                    <h5 class="white-text">More About Us</h5>
                    <p>“Oryx is a business consultancy firm. Since inception in 2004, Oryx has garnered the trust of the market leaders and is constantly unlocking world class best practice to companies. Our unparalleled approach has witnessed success stories from start-ups to growing and sophisticated ones; wholesale distribution, high-end retail items, and professional services to complex manufacturing, engineering and construction operations.” </p>
                </div>
                <div class="col l4 m6 s12">
                    <h5 class="white-text">Contact Information</h5>
                    <p>
                        <i class="material-icons">home</i>
                        101B, Ikeja way, Dolphin Estate, Ikoyi, Lagos
                    </p>
                    <p>
                        <i class="material-icons">contact_phone</i>
                        +234-703-180-6379, +234-706-462-0664
                    </p>
                    <p>
                        <i class="material-icons">phone</i>
                        01-463-0603
                    </p>
                    <p>
                        <i class="material-icons">mail</i>
                        info@oryx-wa.com
                    </p>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container center-align white deep-purple-text">
                © 2018 Oryx West Africa
                <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
            </div>
        </div>
    </footer>


    <environment include="Development">
        <!--Import jQuery before materialize.js-->
        <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
        <script type="text/javascript" src="~/js/materialize.js"></script>
        <script src="~/js/script.js"></script>
        <script type="text/javascript">
            //carousel slide js code
            $('.carousel.carousel-slider').carousel({
                fullWidth: true,
                indicators: true
            });

            $(document).ready(function () {
                M.updateTextFields();
            });
        </script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/js/materialize.js"
                asp-fallback-src="~/lib/materialize/dist/js/materialize.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>